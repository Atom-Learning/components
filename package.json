{
  "name": "@atom-learning/components",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "version": "0.6.0",
  "description": "",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "NODE_ENV=development rollup -c -w",
    "build:lib": "NODE_ENV=production rollup -c && tsc --emitDeclarationOnly",
    "build:docs": "node ./scripts/generate-component-props.mjs && node ./scripts/export-documentation.mjs",
    "start:sandbox": "parcel ./sandbox/index.html --out-dir sandbox/build --open",
    "add-component": "node ./scripts/add-component.mjs",
    "prepublishOnly": "run-s clean build:lib build:docs",
    "clean": "del ./dist/",
    "format": "prettier 'src/**/*.{js,ts,tsx}' --write",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "validate": "run-p validate:**",
    "validate:dependencies": "yarn audit --groups dependencies",
    "validate:linting": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "validate:size": "yarn build:lib && size-limit",
    "validate:types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Atom-Learning/components.git"
  },
  "author": "",
  "license": "ISC",
